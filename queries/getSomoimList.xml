<?xml version="1.0" encoding="UTF-8"?>
<query id="getSomoimList" action="select">
    <tables>
        <table name="somoim" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="status" var="status" default="A" />
    </conditions>
    <order>
        <column name="regdate" order="desc" />
    </order>
</query>